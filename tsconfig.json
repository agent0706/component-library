{
    "compilerOptions": {
        "jsx": "react",
        "module": "commonjs",
        "noImplicitAny": true,
        "outDir": "./dist",
        "preserveConstEnums": true,
        "removeComments": true,
        "sourceMap": true,
        "target": "ES6",
        "esModuleInterop": true,
        "lib": [
            "ES6",
            "DOM"
        ]
    },
    "include": [
        "./components/**/*"
    ]
}